@typeparam T
@inherits MInputSelectBase<T>

<select class="form-control @Cls" id="@Id" style="width:100%" @bind="CurrentValue" >
    <option value=""></option>
    @foreach (var item in Items)
    {
        <option value="@item.Key">
            @item.Value
        </option>
    }
</select>


<CustomValidationMessage For="For" />

